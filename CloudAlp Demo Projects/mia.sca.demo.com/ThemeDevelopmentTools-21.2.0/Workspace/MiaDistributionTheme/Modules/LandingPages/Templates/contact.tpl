<div class="get-in-touch">
  
  <div class="container main-container-content-of-get-in-touch-content">
  <div class="row main-content-get-in-touch">
    <div class="col-sm-6 get-in-touch-main-form">
      <div data-cms-area="get-in-touch-heading-content-cms-area" data-cms-area-filters="path"></div>
	    <form method="POST" class="get-in-touch-form-tag" novalidate >
          <div class="input-field" data-validation="control-group">
		       <div class="firstname-field" data-validation="control">
		          <input type="text" name="firstname" placeholder="*First Name.">
		       </div>
		     </div>
          <div class="input-field" data-validation="control-group">
		       <div class="lastname-field" data-validation="control">
		          <input type="text" name="lastname" placeholder="*Last Name.">
		       </div>
		     </div>
         
		     <div class="input-field" data-validation="control-group">
		       <div class="email-field" data-validation="control">
		          <input type="email" name="email" placeholder="*Email Address.">
		       </div>
		     </div>
		     <div class="input-field" data-validation="control-group">
		       <div class="phone-field" data-validation="control">
		          <input type="text" name="phone" placeholder="*Phone Number.">
		       </div>
		     </div>
		     <div class="input-field" >
		       <div class="business-field" >
		          <input type="text" name="business" placeholder="Business Name.">
		       </div>
		     </div>
		     <div class="input-field" >
		       <div class="comment-field" >
		          <input type="text" name="comments" placeholder="Im Intrested in.">
		       </div>
		     </div>
		     <div class="form-submit-button">
		    <div class="about-us-button1-get-in-touch">
        <div class="about-us-eff-1-get-in-touch"></div>
        <a   class="anchor1-get-in-touch"  type="submit" data-action="validate" > Send </a>
      </div>

      <div class="about-us-button2-get-in-touch">
        <div class="about-us-eff-2-get-in-touch"></div>
        <a   href="vendors" class="anchor2-get-in-touch"> See Vendors </a>
      </div>
		</div>
		     
	    </form>
       <div id="alert-placeholder" >
				    <p class="alert-type" data-type="alert-placeholder"></p>
				 </div>
    </div>
    <div class="col-sm-6 get-in-touch-image-form">
      <div class="get-in-touch-image-field">
        <div data-cms-area="get-in-touch-image-field-in-today-cms-area" data-cms-area-filters="path"></div>
      </div>
    </div>
  </div>

  </div>
   
   <div class="fluid-container-content-become-a-family-tab">
      <div class="container-content-become-a-family-tab container">
        <div class="row become-a-part-of-mia-family-tab-get-in-touch">
          <div class="col-sm-12 become-a-part-of-mia-family-text-content-get-in-touch">
	          <div class="col-sm-6 become-a-part-of-mia-family-text-get-in-touch">
                 <div data-cms-area="become-a-part-of-mia-family-cms-area" data-cms-area-filters="path"></div>
	          </div>
	            <div class="col-sm-6 become-a-part-of-mia-family-button-get-in-touch">
                   <div data-cms-area="get-in-contact-today-cms-area" data-cms-area-filters="path"></div>
	            </div>
          </div>    
	      </div>
      </div>
    </div>
  
   
     <div class="get-in-touch-content-fluid-content container">
        <div data-cms-area="get-in-touch-new-cms-area" data-cms-area-filters="path"></div>
     </div>
</div>