{
	"gulp": {
		"javascript": [
			"JavaScript/*.js",
			"JavaScript/*.ts"
		]
	,	"templates": [
			"Templates/*"
		]
    ,	"images": [
          "Images/*"
        ]
	,	"ssp-libraries": [
			"SuiteScript/*.js"
		]
	,	"autogenerated-services": {
			"Address.Service.ss" : "Address.ServiceController"
		}
	,	"sass": [
			"Sass/**/*.scss"
		]
	,	"unit-test-files": [
			"Tests/*.js"
		]
	,	"unit-test-entry-point": [
			"Tests/UnitTest.Address.js"
		]
	,	"automation-selectors": [
			"Automation/Data/selectors.json"
		]
	,	"automation-helpers" : [
			"Automation/Helpers/*.js"
		]
	,	"automation-tests" : [
			"Automation/Tests/*.js"
		]
	,	"configuration": [
			"Configuration/*.json"
		]
	}
}